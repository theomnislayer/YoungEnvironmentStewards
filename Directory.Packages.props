    <PackageVersion Include="OrchardCore.Application.Cms.Targets" Version="2.1.2" />
    <PackageVersion Include="OrchardCore.ContentManagement" Version="2.1.2" />
    <PackageVersion Include="OrchardCore.ContentTypes.Abstractions" Version="2.1.2" />
